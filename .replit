packages = ["cacert", "cargo", "dnsutils", "freetype", "iputils", "lcms2", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "openssl", "pkg-config", "postgresql", "rustc", "tcl", "tk", "traceroute", "whois", "zlib"]

[deployment]
